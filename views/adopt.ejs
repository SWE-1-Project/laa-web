                        <script>$(document).ready(function() {$('.fancybox').fancybox();});</script>
                        <script>function petZoom(){
                                $(".pet-zoom").elevateZoom({
                                  gallery: 'petThumbs',
                                  galleryActiveClass: "active",
                                  zoomType: "inner",
                                  cursor: "crosshair"
                                });$(".pet-zoom").on("click", function(e) {
                                  var ez = $('.pet-zoom').data('elevateZoom');
                                  $.fancybox(ez.getGalleryList());
                                  return false;
                                });
                                
                                };
                              function petZoomDisable(){
                                if( $(window).width() < 767 ) {
                                  $('.zoomContainer').remove();
                                  $(".pet-zoom").removeData('elevateZoom');
                                  $(".pet-zoom").removeData('zoomImage');
                                } else {
                                  petZoom();
                                }
                              };
        
                              petZoomDisable();
        
                              $(window).resize(function() {
                                petZoomDisable();
                              });
                        </script>
                        <script>

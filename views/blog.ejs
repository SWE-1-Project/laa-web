<%- include('partials/head.ejs') %>
<%- include('partials/header.ejs') %>

<div class="container" style="height: 6em;"></div>
<div id="blog" class="page container blog section-bg">
    <div class="section-title">
        <h2>Blog</h2>
    </div>

    <div class="row">
        <div class="col-12 pb-5">
            <!--SECTION START-->
            <section class="row">
                <!--Start slider news-->
                <div class="col-12 col-md-6 pb-0 pb-md-3 pt-2 pr-md-1">
                    
                    <div id="featured" class="carousel slide carousel" data-ride="carousel">
                        <!--dots navigate-->
                        <ol class="carousel-indicators top-indicator">
                            <li data-target="#featured" data-slide-to="0" class="active"></li>
                            <li data-target="#featured" data-slide-to="1"></li>
                            <li data-target="#featured" data-slide-to="2"></li>
                            <li data-target="#featured" data-slide-to="3"></li>
                        </ol>

                        <!--carousel inner-->
                        <div class="carousel-inner">
                            <!--Item slider-->
                            <%  var counter = 1;
                                for (let a of posts) { %>
                            <div <% if(counter <= 1) { %> class="carousel-item active" <% } else { %> class="carousel-item" <% } %>>
                                <div class="card border-0 rounded-0 text-light overflow zoom">
                                    <div class="position-relative">
                                        <!--thumbnail img-->
                                        <div class="ratio_left-cover-1 image-wrapper">
                                            <a href="#">
                                                <img class="img-fluid w-100" style="height: 29.4em; max-height: 29.4em;"
                                                    src="/assets/img/blog/<%= a.mainImage %>"
                                                    alt="<%= a.title %>">
                                            </a>
                                        </div>
                                        <div class="position-absolute p-2 p-lg-3 b-0 w-100 bg-shadow">
                                            <!--title-->
                                            <a href="#">
                                                <h2 class="h3 post-title text-white my-1"><%= a.title %></h2>
                                            </a>
                                            <!-- meta title -->
                                            <div class="news-meta">
                                                <span class="news-author">by <a class="text-white font-weight-bold"
                                                        href="../category/author.html">Leonard Box</a></span>
                                                <span class="news-date"><%= a.date %></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <% counter++;
                                } %>
                            <!--Item sliders
                            <div class="carousel-item">
                                <div class="card border-0 rounded-0 text-light overflow zoom">
                                    <div class="position-relative">
                                        
                                        <div class="ratio_left-cover-1 image-wrapper">
                                            <a href="https://medium.com/better-programming/how-to-stop-being-afraid-of-technical-interviews-b60b064ff003">
                                                <img class="img-fluid w-100"
                                                    src="/assets/img/blog/interview.png"
                                                    alt="Interview">
                                            </a>
                                        </div>
                                        <div class="position-absolute p-2 p-lg-3 b-0 w-100 bg-shadow">
                                            
                                            <a href="https://medium.com/better-programming/how-to-stop-being-afraid-of-technical-interviews-b60b064ff003">
                                                <h2 class="h3 post-title text-white my-1">How To Stop Being Afraid of Technical Interviews</h2>
                                            </a>
                                            
                                            <div class="news-meta">
                                                <span class="news-author">by <a class="text-white font-weight-bold"
                                                        href="../category/author.html">Joey Colon</a></span>
                                                <span class="news-date">November 22, 2019</span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            
                            <div class="carousel-item">
                                <div class="card border-0 rounded-0 text-light overflow zoom">
                                    <div class="position-relative">
                                        
                                        <div class="ratio_left-cover-1 image-wrapper">
                                            <a href="https://github.com/TAMUSA-ACM/BootStrap-4-and-GitHub-Pages">
                                                <img class="img-fluid w-100"
                                                    src="/assets/img/blog/bootstrap4.png"
                                                    alt="Bootstrap4">
                                            </a>
                                        </div>
                                        <div class="position-absolute p-2 p-lg-3 b-0 w-100 bg-shadow">
                                            
                                            <a href="https://github.com/TAMUSA-ACM/BootStrap-4-and-GitHub-Pages">
                                                <h2 class="h3 post-title text-white my-1">Learn Bootstrap 4</h2>
                                            </a>
                                            
                                            <div class="news-meta">
                                                <span class="news-author">by <a class="text-white font-weight-bold"
                                                        href="../category/author.html">Leonard Box</a></span>
                                                <span class="news-date">June 12, 2020</span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                           
                            <div class="carousel-item">
                                <div class="card border-0 rounded-0 text-light overflow zoom">
                                    <div class="position-relative">
                                        
                                        <div class="ratio_left-cover-1 image-wrapper">
                                            <a href="https://app.joinhandshake.com/events/518429">
                                                <img class="img-fluid w-100"
                                                    src="/assets/img/blog/accenture.png"
                                                    alt="Accenture">
                                            </a>
                                        </div>
                                        <div class="position-absolute p-2 p-lg-3 b-0 w-100 bg-shadow">
                                            
                                            <a href="https://app.joinhandshake.com/events/518429">
                                                <h2 class="h3 post-title text-white my-1">Virtual Externship with Accenture</h2>
                                            </a>
                                            
                                            <div class="news-meta">
                                                <span class="news-author">by <a class="text-white font-weight-bold"
                                                        href="../category/author.html">Mays Center</a></span>
                                                <span class="news-date">July 30, 2020</span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            -->
                        </div>
                        <!--end carousel inner-->
                    </div>

                    <!--navigation-->
                    <a class="carousel-control-prev" href="#featured" role="button" data-slide="prev">
                        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                        <span class="sr-only">Previous</span>
                    </a>
                    <a class="carousel-control-next" href="#featured" role="button" data-slide="next">
                        <span class="carousel-control-next-icon" aria-hidden="true"></span>
                        <span class="sr-only">Next</span>
                    </a>
                </div>
                <!--End slider news-->

                <!--Start box news-->
                <div class="col-12 col-md-6 pt-2 pl-md-1 mb-3 mb-lg-4">
                    <div class="row">
                        <!--news box-->
                        <div class="col-6 pb-1 pt-0 pr-1">
                            <div class="card border-0 rounded-0 text-white overflow zoom">
                                <div class="position-relative">
                                    <!--thumbnail img-->
                                    <div class="ratio_right-cover-2 image-wrapper">
                                        <a href="https://www.tutorialspoint.com/difference-between-stack-and-queue-data-structures">
                                            <img class="img-fluid" src="/assets/img/blog/QUEUE_VS_STACK.png"
                                                alt="Queue V. Stack">
                                        </a>
                                    </div>
                                    <div class="position-absolute p-2 p-lg-3 b-0 w-100 bg-shadow">
                                        <!-- category -->
                                        <a class="p-1 badge badge-primary rounded-0"
                                            href="https://www.tutorialspoint.com/data_structures_algorithms/index.htm">Data Structures</a>

                                        <!--title-->
                                        <a href="https://bootstrap.news/bootstrap-4-template-news-portal-magazine/">
                                            <h2 class="h5 text-white my-1">Implement a Stack as a Queue and vice versa</h2>
                                        </a>
                                    </div>
                                </div>
